{
	"name": "ecommerce-mern-stack-aseeu_org-technopedia",
	"version": "1.0.0",
	"description": "Ecommerce website for MERN course on aseeu.org/technopedia",
	"main": "server.js",
	"scripts": {
		"start-server": "ts-node backend/server.ts",
		"start-server-dev": "ts-node-dev backend/server.ts",
		"start-client": "npm start --prefix frontend",
		"dev": "concurrently \"npm run start-server-dev\" \"npm run start-client\"",
		"data:import": "tsc backend/seeder.ts --outDir backend/dist && node backend/dist/seeder.js",
		"data:destroy": "tsc backend/seeder.ts --outDir backend/dist && node backend/dist/seeder.js -d"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/DreamEcho100/ecommerce-mern-stack-aseeu_org-technopedia.git"
	},
	"keywords": [],
	"author": "Mazen Mohamed Shaban",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/DreamEcho100/ecommerce-mern-stack-aseeu_org-technopedia/issues"
	},
	"homepage": "https://github.com/DreamEcho100/ecommerce-mern-stack-aseeu_org-technopedia#readme",
	"dependencies": {
		"bcryptjs": "^2.4.3",
		"colors": "^1.4.0",
		"cors": "^2.8.5",
		"dotenv": "^10.0.0",
		"express": "^4.17.1",
		"express-async-handler": "^1.2.0",
		"jsonwebtoken": "^8.5.1",
		"mongoose": "^6.0.13"
	},
	"devDependencies": {
		"@types/bcryptjs": "^2.4.2",
		"@types/cors": "^2.8.12",
		"@types/express": "^4.17.13",
		"concurrently": "^6.4.0",
		"ts-node": "^10.4.0",
		"ts-node-dev": "^1.1.8",
		"tslint": "^6.1.3",
		"typescript": "^4.5.2"
	}
}
